(defun maxcol void ((n i32)
                    (a (arr i32 (n n)))
                    (large (arr i32 (n)))
                    (value (arr i32 (n))))
  (do* ((i 0 (+ i 1)))
       ((>= i n))
    (aset 1 large i)
    (aset (aref a i 0) value i)
    (do* ((j 0 (+ j 1)))
         ((>= j n))
      (if (> (aref a i j) (aref value i))
          (progn
            (aset (aref a i j) value i)
            (aset j large i))))))
