(defun maxcol i32 ((n i32)
                   (a (arr i32 (n n)))
                   (large (arr i32 (n)))
                   (value (arr i32 (n))))
  (dotimes (i (0 n))
    (set-arr large 0 i)
    (set-arr value (dabs (aref a 0 i)) i)
    (dotimes (j (1 n))
      (if (> (aref a j i) (aref value i))
          (progn
            (set-arr value i (dabs (aref a j i)))
            (set-arr large i j))))))
